    "order": "fixed",
    "components": [
      "ConsentPage",
      "IntroPage",
      {
        "id": "group-assignment",
        "order": "latinSquare",
        "numSamples": 1,
        "components": [
          {
            "id": "groupA",
            "order": "fixed",
            "components": [
              {
                "order": "fixed",
                "components": [
                  "trial1A", "recognition-question", "identification-question","trial2A"
                ]
              },
              {
                "id": "groupB",
                "order": "fixed",
                "components": [
                  "trial1B","trial2B"
              ]
              },
              {
                "id": "groupC",
                "order": "fixed",
                "components": [
                  "trial1C","trial2C"
                ]
              },
              {
                "id": "groupD",
                "order": "fixed",
                "components": [
                  "trial1D","trial2D"
                ]
              },
              "PostQuestionnaire",
              "EndPage"
            ]
          }
        ]
      }
    ]
  }
