    "recognition-question": {
      "type": "questionnaire",
      "description": "Object recognition",
      "provideFeedback": true,
      "trainingAttempts": 1,
      "response": [
        {
          "id": "recog-q",
          "prompt": "What object did you see in the video",
          "location": "belowStimulus",
          "type": "shortText",
          "placeholder": "Enter your answer here"
        }
      ],
      "correctAnswer": [
        {
          "id": "recog-q",
          "answer": "{{correct_object}}"
        }
      ]
    },

    "identification-question": {
      "type": "questionnaire",
      "description": "Object recognition",
      "response": [
        {
          "id": "id-q",
          "prompt": "What this object real or virtual",
          "location": "belowStimulus",
          "type": "shortText",
          "placeholder": "Enter your answer here"
        }
      ],
      "correctAnswer": [{
        "id": "id-q",
        "answer": "OBJECT"
      }]
    },

    "PostQuestionnaire": {
      "type": "questionnaire",
      "response": [
          {
            "id": "q1",
            "prompt": "Did you experience any problems with our tool?",
            "location": "belowStimulus",
            "type": "longText",
            "placeholder": "Enter your answer here."
          },
          {
            "id": "q2",
            "prompt": "Did anything not render or display properly?",
            "location": "belowStimulus",
            "type": "longText",
            "placeholder": "Enter your answer here"
          },
          {
            "id": "q3",
            "prompt": "Any other issues or anything you would like to tell us?",
            "location": "belowStimulus",
            "type": "longText",
            "placeholder": "Enter your answer here"
          }
        ]
    },
    "EndPage": {
      "type": "markdown",
      "path": "_real_virtual_study/assets/end.md",
      "response": []
    }
