{
  "$schema": "https://raw.githubusercontent.com/revisit-studies/study/v2.3.2/src/parser/StudyConfigSchema.json",
  "studyMetadata": {
    "title": "Real vs Virtual Study Demo",
    "version": "1",
    "authors": ["Leana"],
    "date": "2025-12-02",
    "description": "First Part",
    "organizations": ["inria"]
  },
  "uiConfig": {
    "contactEmail": "",
    "helpTextPath": "_real_virtual_study/assets/observation/help.md",
    "logoPath": "revisitAssets/revisitLogoSquare.svg",
    "withProgressBar": true,
    "autoDownloadStudy": false,
    "withSidebar": false,
    "enumerateQuestions": true
  },
  "components": {
    "IntroPage": {
      "type": "markdown",
      "path": "_real_virtual_study/assets/observation/part1_intro.md",
      "response": []
    },
    "ConsentPage": {
      "type": "markdown",
      "path": "_real_virtual_study/assets/observation/part1_consent.md",
      "response": []
    },

    "PostQuestionnaire": {
      "type": "questionnaire",
      "response": [
        {
          "id": "participant_id",
          "type": "shortText",
          "prompt": "Please enter your participant ID."
        }
      ]
    },
    "EndPage": {
      "type": "markdown",
      "path": "_real_virtual_study/assets/end.md",
      "response": []
    }
  },
  "sequence": {
    "id": "root",
    "order": "fixed",
    "components": [
      "ConsentPage",
      "IntroPage",
      {
        "id": "dynamicBlock",
        "order": "dynamic",
        "functionPath": "src/_real_virtual_study/dynamic.ts#dynamicVideos"
      },
      "PostQuestionnaire",
      "EndPage"
    ]
  }
}